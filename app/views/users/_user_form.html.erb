<%= form_for(@user) do |f| %>
        
<div class="off_white form_options">
        <%= f.label :email %>
        <%= f.email_field :email %>
        </div>

        <div  class="off_white form_options">
        <%= f.label :password %>
        <%= f.password_field :password %>
        </div>

        <div  class="off_white form_options">
        <%= f.label :user_name %>
        <%= f.text_field :user_name %>
        </div>

        <div>
        <%= f.submit class: "top_btn"%>
        </div>

<% end %>