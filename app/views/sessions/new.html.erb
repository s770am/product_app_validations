<h1 class="white center">sign in</h1>

<%= form_for :session, url: sessions_path do |f| %>

        <div  class="off_white form_options">
        <%= f.label :email %>
        <%= f.email_field :email %>
        </div>

        <div  class="off_white form_options">
        <%= f.label :password %>
        <%= f.password_field :password %>
        </div>

        <div>
        <%= f.submit "Login", class: "top_btn"%>
        </div>
<% end %>
  